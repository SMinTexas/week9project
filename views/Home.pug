extend basehome

block base
  .container
    .row
      .col    
        h1 AutoMD - Diagnosis
            h4.spacing Car Model
            hr
            select#carModel.form-control(name='carModel')
                each car in vehicles
                    option= car.model    
            h4.spacing Car Year
            hr
            select#carYear.form-control(name='carYear')
                each car in vehicles
                    option= car.year                 
            h4.spacing Symptoms
            hr
        form(method="POST" action="/Treatment")
            select#symptomname.form-control(name='Symptoms')
                each so in symptoms
                    option(value=so.id)= so.symptom_name                              
            hr              
            button.btn.btn-primary( type='submit') Submit